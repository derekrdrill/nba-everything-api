{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "paths": {
      // "@controllers/*": ["controllers/*"],
      // "@routes/*": ["routes/*"],
      // "@types/*": ["types/*"],
      "@*": ["*"]
    },
  },
  "include": ["src/**/*"]

}